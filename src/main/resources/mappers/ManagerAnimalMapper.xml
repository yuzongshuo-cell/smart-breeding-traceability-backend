        <result column="a_batch_id" property="aBatchId"/>
        <result column="a_hurdles_id" property="aHurdlesId"/>
    </resultMap>

    <!--基础sql字段-->
    <sql id="Base_Column_List">
        a_animal_id
        , a_weight, a_gender, a_healthy, a_status, a_inoculate, a_time, a_batch_id, a_hurdles_id
